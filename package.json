{
  "name": "plush",
  "displayName": "Plush",
  "description": "Plush Experimental Toy Programming Language",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.103.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "plush",
      "aliases": ["Plush", "plush"],
