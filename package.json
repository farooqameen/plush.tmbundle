{
  "name": "plush",
  "displayName": "Plush",
  "description": "Syntax highlighting for Plush using TextMate grammar",
  "version": "1.0.0",
  "repository": "https://github.com/farooqameen/plush.tmbundle.git",
  "publisher": "farooqameen",
  "license": "MIT",

  "engines": {
    "vscode": "^1.103.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "plush",
      "aliases": ["Plush", "plush"],
      "extensions": [".psh"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "plush",
      "scopeName": "source.psh",
      "path": "./syntaxes/plush.tmLanguage.json"
    }]
  }
}
